<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.utah.edu.semmedDao">
    <class name="RecentCitation" table="RECENT_CITATIONS">
        <id name="PMID" column="PMID">
            <generator class="native"/>
        </id>
        <property name="CitationJSON" column="CitationJSON" />
    </class>
</hibernate-mapping>