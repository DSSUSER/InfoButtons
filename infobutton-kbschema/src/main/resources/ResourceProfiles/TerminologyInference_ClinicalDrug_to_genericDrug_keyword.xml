<?xml version="1.0" encoding="UTF-8"?>
<terminologyInference xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../TerminologyInference.xsd">
	<description>Example: Receive RxCUI 310965 [Ibuprofen 200mg Oral Tablet]. Convert to ST "Ibuprofen"</description>
	<codeInference>
		<sourceCodeSystem name="RxNorm" id="RxNorm"/>
		<inferenceDefinition>
			<externalDefinition>
				<inferenceStep direction="DIRECT" sequence="1" maxConcepts="1">
					<codeNavigation>
							<code codeSystem="RxNorm" displayName="HAS_ACTIVE_INGREDIENT" code="HAS_ACTIVE_INGREDIENT"/>
					</codeNavigation>
				</inferenceStep>
			</externalDefinition>
		</inferenceDefinition>
	</codeInference>
</terminologyInference>
